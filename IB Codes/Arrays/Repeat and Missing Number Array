vector<int> Solution::repeatedNumber(const vector<int> &A) {
    vector<int> ans;
    int n=A.size();
    int repeated,missing;
    long long total=0;
    vector<int> m(A.size()+1,0);
    for(int i=0;i<n;i++)
    {
        if(m[A[i]])
        {
            repeated=A[i];
        }
        total+=A[i];
        m[A[i]]=1;
    }
    long long sum=0;
    
    for(int i=1;i<=n;i++)
    {
        sum+=i;
    }
    ans.push_back(repeated);
    ans.push_back(sum-total+repeated);
    return ans;
    
}
