#include<iostream>
#include<stack>
#include<vector>
using namespace std;
#define pb push_back
typedef int ll;
int main()
 {
     int t;
     cin>>t;
     while(t--)
     {
         ll n;
         cin>>n;
         ll arr[n];
         
         for(ll i=0;i<n;i++)
         {
             cin>>arr[i];
         }
         stack<ll> s; 
        vector<ll> v;
         for(int i=0;i<n;i++)
         {
             if(s.empty()) v.pb(1);
            else if(!s.empty()&&arr[s.top()]>arr[i])
                 v.pb(i-s.top());
            else if(!s.empty()&&arr[s.top()]<=arr[i])
                 {
                     while(!s.empty()&&arr[s.top()]<=arr[i])
                     {
                         s.pop();
                     }
                     if(s.empty()){v.pb(i+1);}
                     else{
                     v.pb(i-s.top());}
                 }
             s.push(i);
         }
         
        for(int i=0;i<n;i++)
         {
             cout<<v[i]<<" ";
         }
         cout<<endl;
     }
	
} 
