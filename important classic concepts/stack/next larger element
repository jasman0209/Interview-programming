#include<iostream>
#include<stack>
#include<vector>
using namespace std;
#define pb push_back
typedef long long ll;
int main()
 {
     int t;
     cin>>t;
     while(t--)
     {
         ll n;
         cin>>n;
         ll arr[n];
         
         for(ll i=0;i<n;i++)
         {
             cin>>arr[i];
         }
         stack<ll> s; 
        vector<ll> v;
         for(int i=n-1;i>=0;i--)
         {
             if(s.empty()) v.pb(-1);
            else if(!s.empty()&&s.top()>arr[i])
                 v.pb(s.top());
            else if(!s.empty()&&s.top()<=arr[i])
                 {
                     while(!s.empty()&&s.top()<=arr[i])
                     {
                         s.pop();
                     }
                     if(s.empty()){v.pb(-1);}
                     else{
                     v.pb(s.top());}
                 }
             s.push(arr[i]);
         }
         
        for(int i=n-1;i>=0;i--)
         {
             cout<<v[i]<<" ";
         }
         cout<<endl;
     }
	
} 
