typedef pair<double,pair<int,int>> ppi;
class Solution {
public:
    vector<vector<int>> kClosest(vector<vector<int>>& points, int B) {
         priority_queue<ppi> maxh;
        for(int i=0;i<points.size();i++)
        {
            maxh.push(make_pair(sqrt(points[i][0]*points[i][0]+points[i][1]*points[i][1]),make_pair(points[i][0],points[i][1])));
            if(maxh.size()>B)
            maxh.pop();
        }
    vector<vector<int>> v;
    for(int i=0;i<B;i++)
    {
        ppi t=maxh.top();
        maxh.pop();
        v.push_back({t.second.first,t.second.second});
    }
    return v;
    }
    
};auto speed=[]() { std::ios::sync_with_stdio(false); cin.tie(nullptr); cout.tie(nullptr); return nullptr; }();
