#include<bits/stdc++.h>
using namespace std;
int main()
 {
	int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin >> n;
      int arr[n],dp[n];

      for(int i = 0; i < n; i++){
        cin >> arr[i];
        dp[i]=arr[i];
      }
    int maxans=INT_MIN;
      for(int i= 1; i < n; i++)
      {
          for(int j= 0; j < i; j++)
            {

              if(arr[j]<arr[i])
              {
                  dp[i] = max(dp[i],arr[i] + dp[j]);
              }

          }
          maxans=max(dp[i],maxans);
      }
      cout<<maxans<<endl;

    }

}
