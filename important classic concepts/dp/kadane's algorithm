#include<bits/stdc++.h>
using namespace std;
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int arr[n];
	    int maxsum=INT_MIN;
	    int sum=0;
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    for(int i=0;i<n;i++)
	    {
	        sum+=arr[i];
	        //cout<<sum<<" "<<maxsum<<endl;
	        if(sum>maxsum)
	        {
	            maxsum=sum;
	        }
	        if(sum<0) sum=0;
	    }
	    cout<<maxsum<<endl;
	}
	return 0;
}
