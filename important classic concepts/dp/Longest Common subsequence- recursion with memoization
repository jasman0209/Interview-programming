#include<bits/stdc++.h>
using namespace std;
int dp[102][102];
int lcs(string x,string y, int n, int m)
{
    if(n==0 || m==0) return 0;
    if(dp[n][m]!=-1) return dp[n][m];
    if(x[n-1]==y[m-1])
    {
        dp[n][m]=1+lcs(x,y,n-1,m-1);
        return dp[n][m];
    }
    else
    {
        dp[n][m]=max(lcs(x,y,n-1,m),lcs(x,y,n,m-1));
        return dp[n][m];
    }
}
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b;
    	cin>>a>>b;
    	string x;string y;
    	cin>>x>>y;
    	memset(dp,-1,sizeof(dp));
    	cout<<lcs(x,y,a,b)<<endl;
	}
	
	return 0;
}
