nt Solution::lis(const vector<int> &a) {
    int n=a.size();
    int dp[n];
    dp[0]=1;
    int maxans=1;
    for(int i= 1; i < n; i++)
      {
          //cout<<" b";
          dp[i]=1;
          for(int j= 0; j < i; j++)
            {

              if(a[j]<a[i])
              {
                  dp[i] = max(dp[i],1 + dp[j]);
                  //cout<<dp[i]<<" aa ";
              }


          }
          //cout<<dp[i]<<" ";
          maxans=max(dp[i],maxans);
      }
    return maxans;
}
