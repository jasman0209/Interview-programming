#include<bits/stdc++.h>
int dp[101][101];
using namespace std;
int solvemcm(int arr[],int i,int j)
{
    if(i>=j) return 0;
    if(dp[i][j]!=-1) return dp[i][j];
    int mn=INT_MAX;
    for(int k=i;k<j;k++)
    {
        int tempans=solvemcm(arr,i,k)+solvemcm(arr,k+1,j)+arr[i-1]*arr[k]*arr[j];
        mn=min(mn,tempans);
        dp[i][j]=mn;
    }
    return mn;
}
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int arr[n];
	    memset(dp,-1,sizeof(dp));
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    cout<<solvemcm(arr,1,n-1)<<endl;
	    
	}
	return 0;
}
