#include<iostream>
using namespace std;
int coinchange(int arr[],int n,int w)
{
    if(n==0) return 0;
    
    int dp[n+1][w+1];
	    for(int i=0;i<=n;i++)
	    {
	        dp[i][0]=1;
	    }
	    for(int i=0;i<=w;i++)
	    {
	        dp[0][i]=0;
	    }
	    for(int i=1;i<=n;i++)
	    {
	        for(int j=1;j<=w;j++)
	        {
	            if(arr[i-1]<=j)
	            {
	                dp[i][j]=dp[i][j-arr[i-1]]+dp[i-1][j];
	            }
	            else
	            {
	                dp[i][j]=dp[i-1][j];
	            }
	        }
	    }
	    return dp[n][w];
}
int main()
 {
	
	int t=1;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int arr[n];
	    int w;
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[i];
	    }
	    cin>>w;
	    cout<<coinchange(arr,n,w)<<endl;
	
}
return 0;
}
