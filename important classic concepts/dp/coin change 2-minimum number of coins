#include<bits/stdc++.h>
using namespace std;
int coinchange(int arr[],int n,int w)
{
    if(n==0) return -1;
    int dp[n+1][w+1];
	    for(int i=0;i<=n;i++)
	    {
	        dp[i][0]=0;
	    }
	    for(int i=0;i<=w;i++)
	    {
	        dp[0][i]=INT_MAX-1;
	    }
	    for(int i=0;i<=w;i++)
	    {
	        if(i%arr[0]==0) dp[1][i]=i/arr[0];
	        else dp[1][i]=INT_MAX-1;
	        
	    }
	    for(int i=2;i<=n;i++)
	    {
	        for(int j=1;j<=w;j++)
	        {
	            if(arr[i-1]<=j)
	            {
	                dp[i][j]=min(1+dp[i][j-arr[i-1]],dp[i-1][j]);
	            }
	            else
	            {
	                dp[i][j]=dp[i-1][j];
	            }
	        }
	    }
	    return dp[n][w];
}
int main()
 {
	
	int t=1;
	cin>>t;
	while(t--)
	{
	    int v,n;
	    cin>>v>>n;
	    int coins[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>coins[i];
	    }
	    int a=coinchange(coins,n,v);
	    if(a>INT_MAX-3)
	    cout<<"-1"<<endl;
	    else
	    cout<<a<<endl;
	    
	
}
return 0;
}
