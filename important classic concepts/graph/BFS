// { Driver Code Starts
#include <bits/stdc++.h>

using namespace std;

vector <int> bfs(vector<int> g[], int N);

int main() {
    int T;
    cin >> T;
    while (T--) {
        int N, E;
        cin >> N >> E;
        vector<int> adj[N];
        for (int i = 0; i < E; i++) {
            int u, v;
            cin >> u >> v;
            adj[u].push_back(v);
        }
        vector <int> res = bfs(adj, N);
        for (int i = 0; i < res.size (); i++) cout << res[i] << " ";
        cout << endl;
    }
}// } Driver Code Ends


/* You have to complete this function*/

/* Function to do BFS of graph
*  g[]: adj list of the graph
*  N : number of vertices
*/
 

vector <int> bfs(vector<int> g[], int N) {
bool vis[N]={0};
    // Your code here
    vector<int> ans;
    queue<int> q;
    q.push(0);
    vis[0]=1;

    while(!q.empty())
    {
        int fro=q.front();
        ans.push_back(fro);
        q.pop();
        for(int i=0;i<g[fro].size();i++)
        {
            //cout<<"aaa"<<"   "<<fro<<"    "<<i<<endl;
            if(vis[g[fro][i]]!=1)
            {
                q.push(g[fro][i]);
                vis[g[fro][i]]=1;
            }
        }
    }
    return ans;
}
