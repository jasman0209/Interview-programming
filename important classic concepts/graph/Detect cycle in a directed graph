// { Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


 // } Driver Code Ends


/*  Function to check if the given graph contains cycle
*   V: number of vertices
*   adj[]: representation of graph
*/
bool cyclehelper(bool *visited,vector<int> g[],int i,int parent, bool* recstack)
{
    visited[i]=1;
    recstack[i]=1;
    for(int j=0;j<g[i].size();j++)
    {
        int child=g[i][j];
        if(visited[child]==0)
        {
            if(cyclehelper(visited,g, child,i,recstack)==true)
            return true;
        }
        else
        {
            if(recstack[g[i][j]])
             return true;
        }
    }
    recstack[i]=false;
    return false;
}

bool isCyclic(int V, vector<int> adj[])
{
    // Your code here
    bool *visited=new bool[V]();
    bool *recstack=new bool[V]();
   for(int i=0;i<V;i++)
   {
       if(visited[i]==0)
       {
           bool a=cyclehelper(visited,adj,i,-1,recstack);
           if(a==true)
           return true;
       }
   }
   return false;
   
}

// { Driver Code Starts.

int main() {
	
	int t;
	cin >> t;
	
	while(t--){
	    
	    int v, e;
	    cin >> v >> e;
	    
	    vector<int> adj[v];
	    
	    for(int i =0;i<e;i++){
	        int u, v;
	        cin >> u >> v;
	        adj[u].push_back(v);
	    }
	    
	    cout << isCyclic(v, adj) << endl;
	    
	}
	
	return 0;
}  // } Driver Code Ends
